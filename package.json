{
  "name": "@biconomy/sdk",
  "version": "3.1.0",
  "description": "Main SDK package for Biconomy",
  "main": "./dist/index.ts",
  "typings": "./dist/index.d.ts",
  "keywords": [
    "biconomy",
    "sdk",
    "blockchain",
    "integration",
    "account abstraction",
    "smart accounts",
    "erc-4337",
    "crosschain",
    "cross-chain",
    "metatransactions"
  ],
  "author": "Biconomy (https://biconomy.io)",
  "homepage": "https://github.com/bcnmy/biconomy-client-sdk#readme",
  "license": "MIT",
  "files": [
    "dist/*",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bcnmy/biconomy-client-sdk.git"
  },
  "scripts": {
    "unbuild": "rimraf dist *.tsbuildinfo",
    "build": "rimraf dist && tsc",
    "format": "prettier --write \"{src,tests}/**/*.ts\"",
    "lint": "eslint -c .eslintrc.js 'src/*/**/*.{ts,tsx}'",
    "lint:fix": "eslint -c .eslintrc.js 'src/*/**/*.{ts,tsx}' --fix",
    "test": "concurrently -k --success first 'yarn start:ganache' 'yarn test:run'",
    "start:ganache": "ganache -m 'direct buyer cliff train rice spirit census refuse glare expire innocent quote'",
    "test:run": "yarn jest --runInBand",
    "clean": "rimraf dist ./src/*/*.js.map ./src/*/*.d.ts ./src/*/*.js ./src/*/utils/*.d.ts ./src/*/utils/*.js ./src/*/utils/*.js.map ./src/*/tests/*.js ./src/*/utils/*.js.map ./src/*/tests/*.spec.js.map"
  },
  "bugs": {
    "url": "https://github.com/bcnmy/biconomy-client-sdk/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/debug": "^4.1.12",
    "@types/mocha": "^10.0.6",
    "@types/node": "^17.0.23",
    "@types/node-fetch": "^2.6.9",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "chai": "^4.3.10",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^2.0.0",
    "ganache": "^7.9.1",
    "hardhat": "^2.11.0",
    "jest": "^29.7.0",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@account-abstraction/contracts": "^0.6.0",
    "@account-abstraction/utils": "^0.4.0",
    "@alchemy/aa-core": "^1.2.0",
    "@biconomy-devx/account-contracts-v2": "npm:@biconomy-devx/account-contracts-v2@^1.0.0",
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/abstract-provider": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/logger": "^5.7.0",
    "@ethersproject/properties": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@particle-network/auth": "^0.13.1",
    "@particle-network/biconomy": "^0.1.0",
    "@particle-network/provider": "^0.13.1",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/jest": "^29.5.11",
    "concurrently": "^7.4.0",
    "debug": "^4.3.4",
    "ethereumjs-util": "^7.1.5",
    "ethers": "^5.7.0",
    "loglevel": "^1.8.1",
    "lru-cache": "^10.0.1",
    "merkletreejs": "^0.3.9",
    "node-fetch": "^2.7.0",
    "node-gyp": "^9.4.0",
    "tslint": "^6.1.3",
    "typechain": "^8.1.1",
    "typescript": "^4.6.3",
    "web3-core": "^1.7.1"
  }
}
